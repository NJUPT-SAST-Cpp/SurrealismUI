import { SIcon } from "../icon/index.slint";
export component Carousel inherits Window {
    in property <[image]> sources;
    layout:= HorizontalLayout {
       for source in root.sources:  Rectangle {
        SIcon {
         height: 100%;
         width: 100%;
         source: source;
        }
     }
    }
}

component TestCarousel {
    height: 400px;
    width: 400px;
    Carousel {
        height: 100%;
        width: 100%;
        sources: [
            @image-url("../../README/imgs/sed1.jpeg"),
            @image-url("../../README/imgs/sed2.jpeg")
        ];
    }
}